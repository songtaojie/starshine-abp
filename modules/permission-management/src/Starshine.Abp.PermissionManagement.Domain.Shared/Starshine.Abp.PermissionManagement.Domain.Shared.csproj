<Project Sdk="Microsoft.NET.Sdk">
	<Import Project="..\..\..\..\common.props" />
	
	<PropertyGroup>
		<WarningsAsErrors>Nullable</WarningsAsErrors>
		<AssemblyName>Starshine.Abp.PermissionManagement.Domain.Shared</AssemblyName>
		<PackageId>Starshine.Abp.PermissionManagement.Domain.Shared</PackageId>
		<GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
		<GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
		<GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
		<GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
		<RootNamespace />
	</PropertyGroup>

	<ItemGroup>
		<content Remove="Starshine\Abp\PermissionManagement\Localization\Domain\*.json" />
		<EmbeddedResource Include="Starshine\Abp\PermissionManagement\Localization\Domain\*.json" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Volo.Abp.Validation" Version="$(AbpVersion)" />
		<PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="8.0.11" />
	</ItemGroup>

</Project>
